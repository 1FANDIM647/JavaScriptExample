    <p id=" paragraph1"> I 'm paragraph1</p>
  
    <p id=" paragraph2"> I' m paragraph2 </p>
    
    <p>
         
           <button type="button" onclick="myFunction()">
            Click here
           </button>

    </p>


    <script>
 
    function myFunction( ) {
        
        document.getElementById("paragraph1").innerHTML = " it is ready";
        document.getElementById("paragraph2").innerHTML = " it is ready , second time";
 

    }
    


    </script>
